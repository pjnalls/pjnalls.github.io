{
  "name": "pjnalls.github.io",
  "version": "1.0.0",
  "description": "This is my personal homepage.",
  "repository": {
    "type": "git",
    "url": "https://github.com/pjnalls/pjnalls.github.io"
  },
  "main": "main.js",
  "author": "Preston",
  "license": "ISC",
  "scripts": {
    "webpack-dev": "cross-env NODE_ENV=development webpack --mode development",
    "webpack-prod": "cross-env NODE_ENV=production webpack --mode production",
    "build:dev": "webpack-dev-server --inline --hot --progress --port 8080 --open",
    "build:prod": "npm run build:clean && ngc && npm run webpack-prod && npm run build:clean && mv dist/index.html index.html",
    "build:clean": "rimraf index.html src/**/*.js src/**/*.js.map src/**/*.ngsummary.json src/**/*.metadata.json src/**/**/*.ngfactory.ts src/**/*.ngstyle.ts src/**/*.shim.ts && cp backup/github-calendar.min.js src/vendor/IonicaBizau/github-calendar/dist/github-calendar.min.js",
    "serve": "lite-server",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@ngtools/webpack": "^7.2.3",
    "@types/core-js": "^2.5.0",
    "@types/node": "^10.12.18",
    "angular-router-loader": "^0.8.5",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^1.0.1",
    "compression-webpack-plugin": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "4.0.0-alpha",
    "image-webpack-loader": "^4.6.0",
    "lite-server": "^2.4.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@angular/animations": "^7.2.2",
    "@angular/common": "^7.2.2",
    "@angular/compiler": "^7.2.2",
    "@angular/compiler-cli": "^7.2.2",
    "@angular/core": "^7.2.2",
    "@angular/platform-browser": "^7.2.2",
    "@angular/platform-browser-dynamic": "^7.2.2",
    "@angular/router": "^7.2.2",
    "core-js": "^2.6.3",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.29"
  }
