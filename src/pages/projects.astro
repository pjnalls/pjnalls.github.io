---
import { Image } from 'astro:assets';

import nlpFromScratchImage from '../assets/project-nlp.avif';
import goodCoIndexImage from '../assets/project-goodcoindex.avif';
import youtubeImage from '../assets/project-youtube.avif';
import habitApplitImage from '../assets/project-habit-applit.avif';
import itunesImage from '../assets/project-itunes.avif';
import jobDemandTable from '../assets/project-jd-table.avif';
import jobDemandChart from '../assets/project-jd-chart.avif';

import Layout from '../layouts/BlogPost.astro';
import BaseHead from '../components/BaseHead.astro';
import { SITE_PROJECTS_TITLE, SITE_DESCRIPTION } from '../consts';
import ScrollToTopButton from '../components/ScrollToTopButton';
---

<!doctype html>
<html lang="en">
  <head>
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Roboto+Slab:wght@100..900&family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap"
      rel="stylesheet"
    />
    <script is:inline src="/scripts/fouc-resolver.js"></script>
    <script is:inline src="/scripts/scroll-to-top.js"></script>
    <BaseHead title={SITE_PROJECTS_TITLE} description={SITE_DESCRIPTION} />
    <style>
      a {
        transition: opacity 0.2s ease-in-out !important;
      }
      a:hover {
        opacity: 0.5 !important;
        text-decoration: underline !important;
