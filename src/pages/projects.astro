---
import { Image } from 'astro:assets';

import nlpFromScratchImage from '../assets/project-nlp.avif';
import youtubeImage from '../assets/project-youtube.avif';
import habitApplitImage from '../assets/project-habit-applit.avif';
import itunesImage from '../assets/project-itunes.avif';

import Layout from '../layouts/BlogPost.astro';
import BaseHead from '../components/BaseHead.astro';
import { SITE_PROJECTS_TITLE, SITE_DESCRIPTION } from '../consts';
---

<!doctype html>
<html lang="en">
  <head>
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Roboto+Slab:wght@100..900&family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap"
      rel="stylesheet"
    />
    <script is:inline src="/fouc-resolver.js"></script>
    <BaseHead title={SITE_PROJECTS_TITLE} description={SITE_DESCRIPTION} />
  </head>
  <body>
    <header>
      <script is:inline src="/fouc-resolver.js"></script>
    </header>

    <Layout
      title="My Projects"
      description="What I'm currently building"
      pubDate={new Date('November 17 2024')}
      updatedDate={new Date('November 19, 2024')}
      heroImage="/projects.avif"
    >
      <div>
        <span class="text-5xl" id="project-1"
          ><strong>NLP From Scratch</strong></span
        >
        <br />
        <span class="text-4xl">Ingredients</span>
        <ul>
          <li>• PyTorch</li>
          <li>• NumPy</li>
          <li>• Matplotlib</li>
          <li>• Deep Learning</li>
          <li>• Neural Networks</li>
          <li>• Natural Language Processing (NLP)</li>
          <li>• Python</li>
        </ul>
        <br />
        Code available here:
        <a
          href="https://github.com/pjnalls/ml_projects/tree/main/nlp_from_scratch/"
          target="_blank"
          >https://github.com/pjnalls/ml_projects/tree/main/nlp_from_scratch/</a
        >.
        <br />
        Logo:
        <Image
          src={nlpFromScratchImage}
          alt="Youtube Clone Project"
          loading="lazy"
          width={360}
          height={360}
          class="mx-auto"
        />
        <p class="text-center my-8">
          <a
            href="/projects"
            class="bg-[#d0c2ff] dark:bg-[#6e408a] border-2 border-[#6e408a] dark:border-[#d0c2ff] text-white px-4 py-2 rounded-md mb-8"
            style="text-decoration: none;">Scroll to Top ⬆</a
          >
        </p>
        <br />
        <span class="text-5xl" id="projects"
          ><strong>Cross-Platform Apps I've Built</strong></span
        >
        <br />
        <ul class="mb-24">
          <li><a href="#project-1">• YouTube Clone</a></li>
          <li><a href="#project-2">• Habit AppLit</a></li>
          <li><a href="#project-3">• Classic iTunes</a></li>
        </ul>
        <span class="text-5xl" id="project-1"
          ><strong>YouTube Clone</strong></span
        >
        <br />
        <span class="text-4xl">Ingredients</span>
        <ul>
          <li>• React Native</li>
          <li>• Expo</li>
          <li>• Expo Router</li>
          <li>• YouTube Data API</li>
          <li>• TypeScript</li>
        </ul>
        <br />
        Code available here:
        <a href="https://github.com/pjnalls/youtube/" target="_blank"
          >https://github.com/pjnalls/youtube/</a
        >.
        <br />
        Screens:
        <Image
          src={youtubeImage}
          alt="Youtube Clone Project"
          loading="lazy"
          width={1370}
          height={1604}
        />
        <p class="text-center my-8">
          <a
            href="/projects"
            class="bg-[#d0c2ff] dark:bg-[#6e408a] border-2 border-[#6e408a] dark:border-[#d0c2ff] text-white px-4 py-2 rounded-md mb-8"
            style="text-decoration: none;">Scroll to Top ⬆</a
          >
        </p>
        <br />
        <span class="text-5xl" id="project-2"
          ><strong>Habit AppLit</strong></span
        >
        <br />
        <span class="text-4xl">Ingredients</span>
        <ul>
          <li>• React Native</li>
          <li>• React Native Gifted Charts</li>
          <li>• React Native Linear Gradient</li>
          <li>• Expo</li>
          <li>• Expo Router</li>
          <li>• AsyncStorage</li>
          <li>• TypeScript</li>
        </ul>
        <br />
        Code available here:
        <a href="https://github.com/pjnalls/habit-applit/" target="_blank"
          >https://github.com/pjnalls/habit-applit/</a
        >.
        <br />
        Screens:
        <Image
          src={habitApplitImage}
          alt="Habit AppLit Project"
          loading="lazy"
          width={1370}
          height={1616}
        />
        <p class="text-center my-8">
          <a
            href="/projects"
            class="bg-[#d0c2ff] dark:bg-[#6e408a] border-2 border-[#6e408a] dark:border-[#d0c2ff] text-white px-4 py-2 rounded-md mb-8"
            style="text-decoration: none;">Scroll to Top ⬆</a
          >
        </p>
        <br />
        <span class="text-5xl" id="project-3"
          ><strong>Classic iTunes</strong></span
        >
        <br />
        <span class="text-4xl">Ingredients</span>
        <ul>
          <li>• React Native</li>
          <li>• React Native Track Player</li>
          <li>• Shaka Player</li>
          <li>• Expo</li>
          <li>• TypeScript</li>
        </ul>
        <br />
        Code available here:
        <a href="https://github.com/pjnalls/itunes/" target="_blank"
          >https://github.com/pjnalls/itunes/</a
        >.
        <br />
        Screens:
        <Image
          src={itunesImage}
          alt="iTunes Clone Project"
          loading="lazy"
          width={1370}
          height={803}
        />
        <p class="text-center my-8">
          <a
            href="/projects"
            class="bg-[#d0c2ff] dark:bg-[#6e408a] border-2 border-[#6e408a] dark:border-[#d0c2ff] text-white px-4 py-2 rounded-md mb-8"
            style="text-decoration: none;">Scroll to Top ⬆</a
          >
        </p>
        <br />
        <p class="text-center">
          Please also <a href="https://github.com/pjnalls/" target="_blank"
            >visit my GitHub profile</a
          > to view what I'm currently working on.
        </p>
      </div>
    </Layout>
  </body>
</html>

<style>
  a {
    color: inherit;
  }
</style>